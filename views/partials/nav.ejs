<div class="nav-bar">
  <div class="nav-home-cont">
    <a href="/">Home</a>
  </div>
  <div class="nav-login-cont">
    <div class="login-cont login-item">
      <% if (currentUser) { %>
        <a href="/log-out">Log Out</a>
          <% if (!currentUser.admin) { %>
            <span class="space-span">|</span>
          <% } %>
      <% } else { %>
        <a href="/log-in">Log In</a><span class="space-span">|</span>
      <% } %>
    </div>
    <% if (!currentUser) { %>
      <div class="signup-cont login-item">
        <a href="/sign-up">Sign Up</a> 
      </div>
    <% } %>
    <% if (currentUser && !currentUser.member) { %>
      <div class="join-cont login-item">
        <a href="/join-club">Join Club</a><span class="space-span">|</span>
      </div>
    <% } %>
    <% if (currentUser && !currentUser.admin) { %>
      <div class="admin-cont login-item">
        <a href="/become-admin">Become Admin</a>
      </div>
    <% } %>
  </div>
</div>